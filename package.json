{
  "name": "csslib-dev-tool",
  "version": "3.1.2",
  "description": "a tool for building css lib",
  "main": "build/webpack.config.js",
  "scripts": {
    "study": "webpack --config build/webpack.config.js --json --profile > stats.json",
    "dev:webpack4": "cross-env NODE_ENV=development webpack --config build/webpack.config.js",
    "pro:webpack4": "cross-env NODE_ENV=production webpack --config build/webpack.config.js  --progress --hide-modules",
    "dev:simple-server": "cross-env SERVER_TYPE=simple webpack-dev-server --config build/webpack.config.js",
    "dev:complex-server": "cross-env SERVER_TYPE=complex node build/dev-server.js",
    "dev:custom-server": "nodemon --watch build/dev-server.js --watch build/webpack.config.js --exec \"cross-env SERVER_TYPE=complex node build/dev-server.js\"",
    "dev": "npm run dev:complex-server",
    "pro": "npm run pro:webpack4",
    "lint": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YMC-GitHub/cssLib-dev-tool.git"
  },
  "keywords": [
    "css",
    "lib",
    "dev",
    "webpack",
    "ymc"
  ],
  "author": "yemiancheng <ymc.github@gmail.com> (https://github.com/YMC-GitHub)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "balloon-css": "git+https://github.com/kazzkiq/balloon.css.git",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^3.2.0",
    "doiuse": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-yemiancheng": "^1.0.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.22.2"
  },
  "files": [
    "build/webpack.config.js",
    "package.json",
    "README.md",
    "build/dev-server.js",
    "build/dev-client.js",
    "build/page-reload.js",
    "build/config.js"
  ]
}
